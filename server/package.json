{
  "name": "server",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16 <17",
    "npm": ">=8 <9"
  },
  "scripts": {
    "migrate:make": "npx knex migrate:make",
    "migrate:rollback": "npx knex migrate:rollback",
    "migrate:run": "npx knex migrate:latest",
    "seed:run": "npx knex seed:run",
    "start": "nodemon src/app.js",
    "lint:js": "npx eslint --ext .js src",
    "lint": "npm run lint:js",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --runInBand --detectOpenHandles --forceExit",
    "test:starter": "npm run test -- --verbose --rootDir=tests/api/starter/",
    "test:dislike-post": "npm run test -- --verbose --rootDir=tests/api/1-dislike-post/",
    "test:update-post": "npm run test -- --verbose --rootDir=tests/api/2-update-post/",
    "test:delete-post": "npm run test -- --verbose --rootDir=tests/api/3-delete-post/",
    "test:update-comment": "npm run test -- --verbose --rootDir=tests/api/4-update-comment/",
    "test:delete-comment": "npm run test -- --verbose --rootDir=tests/api/5-delete-comment/"
  },
  "dependencies": {
    "@fastify/cors": "7.0.0",
    "@fastify/static": "5.0.2",
    "axios": "0.27.2",
    "bcrypt": "5.0.1",
    "dotenv": "16.0.1",
    "fastify": "3.29.0",
    "fastify-multer": "2.0.2",
    "fastify-plugin": "3.0.1",
    "form-data": "4.0.0",
    "jsonwebtoken": "8.5.1",
    "knex": "2.0.0",
    "nodemon": "2.0.16",
    "objection": "3.0.1",
    "pg": "8.7.3",
    "pg-hstore": "2.3.4",
    "qs": "6.10.3",
    "shared": "file:../shared",
    "socket.io": "4.5.0"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@jest/globals": "29.3.1",
    "@types/jest": "29.2.6",
    "eslint-config-airbnb-base": "15.0.0",
    "jest": "29.3.1",
    "npm": "9.3.1",
    "pino-pretty": "7.6.1"
  }
}
