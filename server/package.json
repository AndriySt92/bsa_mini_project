{
  "name": "server",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16 <17",
    "npm": ">=8 <9"
  },
  "scripts": {
    "migrate:make": "npx knex migrate:make",
    "migrate:rollback": "npx knex migrate:rollback",
    "migrate:run": "npx knex migrate:latest",
    "seed:run": "npx knex seed:run",
    "start": "nodemon src/server.js",
    "lint:js": "npx eslint --ext .js src",
    "lint": "npm run lint:js"
  },
  "dependencies": {
    "@fastify/cors": "8.2.0",
    "@fastify/static": "6.6.1",
    "axios": "1.2.3",
    "bcrypt": "5.1.0",
    "dotenv": "16.0.3",
    "fastify": "4.12.0",
    "fastify-multer": "2.0.3",
    "fastify-plugin": "4.5.0",
    "form-data": "4.0.0",
    "jsonwebtoken": "9.0.0",
    "knex": "2.4.2",
    "nodemon": "2.0.20",
    "objection": "3.0.1",
    "pg": "8.8.0",
    "pg-hstore": "2.3.4",
    "qs": "6.11.0",
    "socket.io": "4.5.4",
    "shared": "file:../shared"
  },
  "devDependencies": {
    "eslint-config-airbnb-base": "15.0.0",
    "pino-pretty": "9.1.1"
  }
}
